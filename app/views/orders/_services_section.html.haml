.input-fields{:style => "", :class => "col s12"}
  %br
    =image_tag "Tooth-icon.png", :style => "font-size:46px; width: 24px; margin-right: 24px;", :class => "material-icons"
    = f.check_box :antagonista
    = f.label :antagonista, "Antagonista", :style => "padding-right:15px;"
    = f.check_box :foto
    = f.label :foto, "Foto", :style => "margin-left: 52px;"
    %br
    = f.check_box :mordida
    = f.label :mordida, "Mordida", :style => "margin-left: 52px;"

.input-field.col.s12
  %i.material-icons.prefix library_add
  = f.label :quantity, "Cantidad/Unidad"
  = f.number_field :quantity, :class => 'validate'
.input-field
  = f.hidden_field :state




.input-fields{:class => "col m12 s6"}
  %ul.collapsible{"data-collapsible" => "accordion"}
    %li
      .collapsible-header
        %i.material-icons> keyboard_arrow_right
        Tipo de Trabajo
      .collapsible-body
        %p
        = f.fields_for :services do |s|
          = s.check_box :corona
          = s.label :corona
          = s.check_box :puente
          = s.label :puente




